{
  "manifest_version": 3,
  "name": "ECE150 Catppuccin Mocha DS",
  "version": "1.0",
  "description": "Catppuccin Mocha theme for ECE150 and ECE204 lecture materials",
  "permissions": ["activeTab", "scripting"],
  "content_scripts": [
    {
      "matches": [
        "https://ece.uwaterloo.ca/~ece150/*",
        "https://ece.uwaterloo.ca/~dwharder/nm/Lecture_materials/*"
      ],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ],
  "icons": {
    "128": "logo.png"
  }
}

